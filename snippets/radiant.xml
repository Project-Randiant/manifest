<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="radiant"
           fetch="https://github.com/ProjectRadiant" 
           revision="eleven" />

    <remote name="pixel"
          fetch="https://github.com/PixelExperience"
          revision="eleven"
          review="gerrit.pixelexperience.org" />

      <remote name="dababy"
          fetch="https://github.com/altorgtohostshit/"
          revision="eleven" />
            
  <remote  name="fluid"
           fetch="https://github.com/Project-Fluid" 
           revision="fluid-11" />

  <remote name="nezuko-devices"
          fetch="https://github.com/NezukoOS-Devices"
          revision="eleven" />

  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote name="dot-gitlab"
          fetch="http://gitlab.com/dotos-official/"
          review="https://review.droidontime.com/"
          revision="refs/heads/dot11" />

  <!-- Rootdir -->
  <project path="bionic" name="bionic" remote="radiant" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" remote="fluid" />

  <!-- Build -->
  <project path="build/make" name="build" remote="radiant" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" remote="radiant" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/radiant/sepolicy" name="device_radiant_sepolicy" remote="radiant" />
  <project path="device/qcom/sepolicy" name="device_qcom_sepolicy" remote="radiant" revision="eleven" />
  <project path="device/qcom/sepolicy_vndr" name="device_qcom_sepolicy_vndr" revision="eleven" remote="radiant" />
  <project path="device/qcom/sepolicy-legacy" name="device_qcom_sepolicy" revision="eleven-legacy" remote="radiant" />
  <project path="device/qcom/sepolicy-legacy-um" name="device_qcom_sepolicy" revision="eleven-legacy-um" remote="radiant" />

  <!-- External -->
  <project path="external/selinux" name="external_selinux" remote="radiant" />
  <project path="external/tinycompress" name="external_tinycompress" remote="radiant" />
  <project path="external/asus/stitchimage" name="external_asus_stitchimage" remote="radiant"/>
  <project path="external/faceunlock" name="external_faceunlock" remote="dababy" />
  <project path="external/jemalloc_new" name="external_jemalloc_new" remote="radiant" />
  <project path="external/arm-optimized-routines" name="external_arm-optimized-routines" remote="radiant" />

  <!-- Frameworks -->
  <project path="frameworks/base" name="frameworks_base" remote="radiant" />
  <project path="frameworks/native" name="frameworks_native" remote="radiant" />
  <project path="frameworks/av" name="frameworks_av" remote="radiant" />
  <project path="frameworks/opt/telephony" name="frameworks_opt_telephony" remote="radiant" />
  <project path="frameworks/libs/systemui" name="frameworks_libs_systemui" remote="radiant" />

  <!-- Hardware -->
  <project path="hardware/lineage/interfaces" name="hardware_lineage_interfaces" remote="radiant" />
  <project path="hardware/interfaces" name="hardware_interfaces" remote="radiant" />
  <project path="prebuilts/sdk" name="android_prebuilts_sdk" remote="dot-gitlab" />

  <!-- Packages -->
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="radiant" />
  <project path="packages/services/Telephony" name="packages_services_Telephony" remote="radiant" />
  <project path="packages/apps/ThemePicker" name="packages_apps_ThemePicker" remote="radiant" />
  <project path="packages/apps/Launcher3" name="packages_apps_Launcher3" remote="radiant" />
  <project path="packages/apps/PermissionController" name="packages_apps_PermissionController" remote="radiant" />
  <project path="packages/apps/Styles" name="packages_apps_Styles" remote="radiant" />
  <project path="packages/apps/WallpaperPicker2" name="packages_apps_WallpaperPicker2" remote="radiant" />
  <project path="packages/overlays/Themes" name="packages_overlays_Themes" remote="radiant" />
  <project path="packages/apps/MotoSignatureApp" name="NemesisDevelopers/motorola/motorola_motosignatureapp" remote="gitlab" revision="eleven"/>
  <project path="packages/apps/Themer" name="packages_apps_Themer" remote="radiant"/>
  <project path="packages/apps/DocumentsUI" name="packages_apps_DocumentsUI" remote="fluid" />
  <project path="packages/apps/FaceUnlockService" name="packages_apps_FaceUnlockService" remote="pixel" />
  <project path="packages/apps/Plugins" name="packages_apps_Plugins" remote="radiant" />
  <project path="packages/apps/RadiantExtras" name="packages_apps_RadiantExtras" remote="radiant" />

  <project path="packages/apps/Snap" name="packages_apps_Snap" remote="radiant" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="radiant" />
  <project path="packages/apps/Dialer" name="packages_apps_Dialer" remote="radiant" />
  <project path="packages/apps/Updater" name="packages_apps_Updater" remote="radiant" />

  <!-- System -->
  <project path="system/core" name="system_core" remote="radiant" />
  <project path="system/vold" name="system_vold" remote="fluid" />
  <project path="system/sepolicy" name="system_sepolicy" remote="radiant" />
  <project path="system/security" name="system_security" remote="radiant" />
  <project path="system/netd" name="system_netd" remote="fluid" />
  <project path="system/tools/mkbootimg" name="system_tools_mkbootimg" remote="fluid" />
  <project path="system/update_engine" name="system_update_engine" remote="fluid" />

  <!-- Vendor -->
  <project path="vendor/radiant" name="vendor_radiant" remote="radiant" />
  <project path="vendor/nezextras" name="vendor_nezextras" remote="radiant" />
  <project path="vendor/support" name="vendor_support" remote="radiant" />
  <project path="vendor/gapps" name="Maitreya29/vendor_gapps" remote="gitlab" revision="radiant"/>

</manifest>
